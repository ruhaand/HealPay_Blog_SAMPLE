{
  "dependencies": {
    "@popperjs/core": "^2.11.4",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "esbuild": "^0.14.27",
    "install": "^0.13.0",
    "jquery": "^3.6.0",
    "popper": "^1.0.1",
    "sass": "^1.49.9"
  },
  "scripts": {
